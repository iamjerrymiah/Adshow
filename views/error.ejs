<%- include('includes/stripe-header') %>


            <div class="section post-section pt-5" style="background-color: white; text-align: center;">
                    <div class="container">
						<div class="row justify-content-center">
							<img src="images/error.gif" alt="" style="margin-top: -45px; min-width: 100%; height: 410px;">
                    
                            <h1 style="font-weight: bold; color: red; margin-bottom: 20px;">
                             Something Went Wrong! <img src="images/oops-emoji.png" alt="" style="max-width: 30%; height: 80px;"></h1>
					
								<h6 style="margin-bottom: 20px; font-size: large;">
							<%= message %>
								</h6>

								<% if(message !== 'You are not logged in! Please log in to get access.'){ %>
									<a href="/" class="btn btn-primary">Home Page</a>
								<% } %>	
								
								<% if(message === 'You are not logged in! Please log in to get access.'){ %>
									<a href="/login" class="btn btn-primary">Log In</a>
								<% } %>	
				</div>

			</div>
            </div>







